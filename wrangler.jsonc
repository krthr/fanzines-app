{
  "$schema": "node_modules/wrangler/config-schema.json",

  "compatibility_flags": ["nodejs_compat", "no_nodejs_compat_v2"],

  "observability": {
    "logs": {
      "enabled": true,
      "invocation_logs": true,
    },
    "traces": {
      "enabled": true,
    },
  },

  "r2_buckets": [
    {
      "binding": "BLOB",
      "bucket_name": "fanzines-app-assets",
    },
  ],
}
